 <odoo>
    <record id="dvr_ezviz_view_tree" model="ir.ui.view">
    <field name="name">alarms.comunicator.tree</field>
    <field name="model">alarms.comunicator</field>
        <field name="arch" type="xml">
            <tree>
                <!-- Add All Fields You Want In The List Here -->
                <field name="com_model"/>
                <field name="com_serial"/>
            </tree>
        </field>
    </record>

    <record id="dvr_ezviz_view_form" model="ir.ui.view">
        <field name="name">dvr.ezviz.form</field>
        <field name="model">alarms.comunicator</field>
        <field name="arch" type="xml">
                <form string="">
				<group>
                    <field name="com_model" colspan="12"/>
                    <field name="com_serial" colspan="12"/>
                    <field name="com_password" colspan="12"/>
                </group>
				</form>
        </field>

    </record>


 </odoo> 
